param1 = 3;
param2 = 6;

round( (param1+param2)*(1+(param1^2 + param2^2)^(1/2)/2) , 3)
%round(-1 / sqrt(2*param2), 3)
%round(1 /(2 * sqrt(param1*param2)), 3)

function factorial(n):
    if n == 0:
        return 1              // базовое условие
    else:
        return n * factorial(n - 1)  // рекурсивный вызов
